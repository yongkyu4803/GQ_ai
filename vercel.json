{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/prompt/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/genpro/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/news/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/bill/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}